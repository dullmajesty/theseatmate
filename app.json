{
  "expo": {
    "name": "TheSeatmate",
    "slug": "TheSeatmate",
    "scheme": "com.theseatmate.app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SeatMate.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/SeatMate.png",
      "resizeMode": "contain",
      "backgroundColor": "#201B51"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SeatMate.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/SeatMate.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
